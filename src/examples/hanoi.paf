program hanoi;

type
  Pole = Array of Integer;

var
  @n : Integer;
  @x, @y, @z : Pole;
  
procedure hanoi(n: integer; var x, y, z: Pole);
begin
  if n = 1 then
     move_node(x, 1, z)
  else begin
     hanoi(n - 1, x, z, y);
     move_node(x, n, z);
     hanoi(n - 1, y, x, z);
  end;
end;

procedure move_node(var x: Pole; n: Integer; var y: Pole);
begin
  x[n -1] := None;
  y[n - 1] := n;
end;

begin
  hanoi(n, x, y, z);  
end.
